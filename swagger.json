{
    "swagger": "2.0",
    "info": {
      "title": "Food Delivery API",
      "description": "API for calculating food delivery prices based on various factors.",
      "version": "1.0.0"
    },
    "basePath": "/api",
    "schemes": ["http"],
    "paths": {
      "/pricing/calculate-price": {
        "post": {
          "summary": "Calculate total price for food delivery",
          "description": "Calculate the total price of food delivery based on zone, organization, distance, and item type.",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "description": "Request body",
              "required": true,
              "schema": {
                "type": "object",
                "properties": {
                  "zone": {
                    "type": "string"
                  },
                  "organizationId": {
                    "type": "number"
                  },
                  "totalDistance": {
                    "type": "number"
                  },
                  "itemType": {
                    "type": "string"
                  }
                },
            "example": {
                    "zone": "central",
                    "organizationId": 1,
                    "totalDistance": 12,
                    "itemType": "perishable"
                  }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Successful response",
              "schema": {
                "type": "object",
                "properties": {
                  "total_price": {
                    "type": "number"
                  }
                }
              }
            },
            "400": {
              "description": "Bad request",
              "schema": {
                "type": "object",
                "properties": {
                  "error": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Not found",
              "schema": {
                "type": "object",
                "properties": {
                  "error": {
                    "type": "string"
                  }
                }
              }
            },
            "500": {
              "description": "Internal server error",
              "schema": {
                "type": "object",
                "properties": {
                  "error": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  